lread orgImg %p1

//////////////////////////////////////////
// Convert RBG to HSV and extract Value //
//////////////////////////////////////////
Convert orgImg %p2 HSV888Image
Extract1HSVchannel %p2 %p2 Value Int32Image

//////////////////////////////////////////
/////////////// Blur Image ///////////////
//////////////////////////////////////////
GaussianFilter %p2 1 6

//////////////////////////////////////////
///////// Canny Edge Detection ///////////
//////////////////////////////////////////
Canny %p2 %p2 1 1 80 EightConnected


